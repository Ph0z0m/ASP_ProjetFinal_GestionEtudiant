@using GestionEtudiants.Models

@model AcceuilViewModel



<div class="text-center">
    <h1 class="display-4">Page d'accueil</h1>
    <h2>Les Etudiants</h2>
    <table class="table">
        <tr>
            <td>Identifiant</td>
            <td>Nom</td>
            <td>Prenom</td>
            <td>Genre</td>
            <td>Age</td>
            <td>Promotion</td>
            <td>Identifiant Promotion</td>
        </tr>


        @foreach (Etudiant etudiant in Model.Etudiants)
        {
            <tr>
                <td>@etudiant.Identifiant</td>
                <td>@etudiant.Nom</td>
                <td>@etudiant.Prenom</td>
                <td>@etudiant.Genre</td>
                <td>@etudiant.Age</td>
                <td>@etudiant.Promotion</td>
                <td>@etudiant.IdPromotion</td>
            </tr>
        }
    </table>

</div>
